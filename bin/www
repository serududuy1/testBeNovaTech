const http = require("http");
const app = require("../app");

const server = http.createServer(app);
// dpt.config();

server.listen(process.env.PORT || 8000, function () {
  console.log(
    "aserver listening on port http://localhost:%d in %s mode",
    this.address().port,
    app.settings.env
  );
});

module.exports = server;
